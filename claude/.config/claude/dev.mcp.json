{
  "mcpServers": {
    "aws-knowledge": {
      "type": "http",
      "url": "https://knowledge-mcp.global.api.aws"
    },
    "codex": {
      "type": "stdio",
			"command": "codex",
      "args": [
        "mcp-server"
      ],
      "env": {}
		},
    "gemini": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "mcp-gemini-cli",
        "--allow-npx"
      ],
      "env": {}
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        ".",
        "--enable-web-dashboard=false"
      ],
      "cwd": "."
    }
  }
}
